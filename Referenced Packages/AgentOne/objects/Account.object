    <validationRules>
        <fullName>Business_Cannot_Modify_Classification</fullName>
        <active>true</active>
        <description>If the AccountType is chosen as &quot;Client&quot; or &quot;Prospect&quot;, we cannot switch it to another type.</description>
        <errorConditionFormula>AND(
        NOT(ISPICKVAL( AgentOneAccountType__c, &apos;Client&apos;)),ISCHANGED( AgentOneAccountType__c ),  AccountLockClassification__c  == true
        )</errorConditionFormula>
        <errorDisplayField>AgentOneAccountType__c</errorDisplayField>
        <errorMessage>Once the classification is set to client it cannot be changed.</errorMessage>
    </validationRules>
